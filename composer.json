{
  "name": "byjg/uri",
  "type": "project",
  "description": "A PHP implementation of PSR-7 UriInterface with additional utility methods",
  "autoload": {
    "psr-4": {
      "ByJG\\Util\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.3 <8.6",
    "psr/http-message": "^1.0|^1.1|^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "vimeo/psalm": "^5.9|^6.13"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "provide": {
    "psr/http-message-implementation": "1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --threads=1"
  },
  "license": "MIT"
}
